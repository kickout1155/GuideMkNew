CREATE TABLE HeroesDb (
  id TEXT NOT NULL PRIMARY KEY,
  name TEXT NOT NULL,
  urlImage TEXT NOT NULL
);

selectAll:
SELECT * FROM HeroesDb;

insertObject:
INSERT INTO HeroesDb(id, name,urlImage)
VALUES?;

updateHeroesDb:
UPDATE HeroesDb
SET id = ?,name=?,urlImage = ?
WHERE id = ?;

clearDb:
DELETE FROM HeroesDb;